  "scripts": {
    "start": "craco start",
    "optimize-images": "node scripts/convert-images.js",
    "build": "npm run optimize-images && craco build && react-snap",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "ANALYZE=true npm run build"
  },
  "reactSnap": {
    "source": "build",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "include": [
      "/",
      "/news",
      "/announcements"
    ],
    "puppeteer": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
